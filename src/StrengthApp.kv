<ORMPredictorScreen>:
    name: "1rm"
    BoxLayout:
        orientation: "vertical"
        size_hint: 0.75, 0.75
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        BoxLayout:
            size_hint: 1, 0.075
            Label:
                text: "Weight Lifted: "
                text_size: self.width, None
                halign: "right"
                bold: True
                size_hint: 0.4, 1
            TextInput:
                id: weight
                input_filter: "float"
                hint_text: "Weight (lb)"
                size_hint: 0.6, 1
        BoxLayout:
            size_hint: 1, 0.075
            Label:
                text: "Reps Performed: "
                text_size: self.width, None
                halign: "right"
                bold: True
                size_hint: 0.4, 1
            TextInput:
                id: reps
                input_filter: "int"
                hint_text: "Number of Reps (1 to 10)"
                size_hint: 0.6, 1
        Button:
            text: "Calculate 1RM!"
            bold: True
            pos_hint: {"center_x": 0.5}
            size_hint: 0.5, 0.075
            on_release: root.estimate()
            disabled: True if weight.text == "" or reps.text == "" or not (int(reps.text) >= 1 and int(reps.text) <= 10) else False
        Label:
            text: "Estimated 1RM: " + str(root.e)
            bold: True
            size_hint: 1, 0.1
        GridLayout:
            size_hint: 1, 0.625
            pos_hint: {"center_x": 0.5}
            rows: 5
            cols: 2
            Label:
                text: "50% 1RM: " + str(round(int(root.e)*0.5))
                bold: True
            Label:
                text: "75% 1RM: " + str(round(int(root.e)*0.75))
                bold: True
            Label:
                text: "55% 1RM: " + str(round(int(root.e)*0.55))
                bold: True
            Label:
                text: "80% 1RM: " + str(round(int(root.e)*0.8))
                bold: True
            Label:
                text: "60% 1RM: " + str(round(int(root.e)*0.6))
                bold: True
            Label:
                text: "85% 1RM: " + str(round(int(root.e)*0.85))
                bold: True
            Label:
                text: "65% 1RM: " + str(round(int(root.e)*0.65))
                bold: True
            Label:
                text: "90% 1RM: " + str(round(int(root.e)*0.90))
                bold: True
            Label:
                text: "75% 1RM: " + str(round(int(root.e)*0.75))
                bold: True
            Label:
                text: "95% 1RM: " + str(round(int(root.e)*0.95))
                bold: True
    Button:
