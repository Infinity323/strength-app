#:kivy 2.1.0
MainMenu:
    MainMenuScreen:
    AboutScreen:
    NippardStrengthStandardScreen:
    PoundsBreakdownScreen:
    KilosBreakdownScreen:
    PoundsPlateScreen:
    KilosPlateScreen:
    ORMPredictorScreen:

<MainMenuScreen>:
    name: "main"
    BoxLayout:
        size_hint: 0.75, 0.75
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        orientation: "vertical"
        Label:
            text: "General Strength Application\n(ver 1.0.0)"
            text_size: self.width, None
            halign: "center"
            bold: True
        Button:
            text: "Jeff Nippard's Strength Level Progress Tester"
            text_size: self.width, None
            halign: "center"
            bold: True
            on_release:
                app.root.current = "nippard"
                root.manager.transition.direction = "left"
        BoxLayout:
            Button:
                text: "Weight Breakdown\n(POUNDS)"
                text_size: self.width, None
                halign: "center"
                bold: True
                on_release:
                    app.root.current = "pounds"
                    root.manager.transition.direction = "left"
            Button:
                text: "Weight Breakdown\n(KILOGRAMS)"
                text_size: self.width, None
                halign: "center"
                bold: True
                on_release:
                    app.root.current = "kilos"
                    root.manager.transition.direction = "left"
        BoxLayout:
            Button:
                text: "Plate Totals\n(POUNDS)"
                text_size: self.width, None
                halign: "center"
                bold: True
                on_release:
                    app.root.current = "poundsplates"
                    root.manager.transition.direction = "left"
            Button:
                text: "Plate Totals\n(KILOGRAMS)"
                text_size: self.width, None
                halign: "center"
                bold: True
                on_release:
                    app.root.current = "kilosplates"
                    root.manager.transition.direction = "left"
        Button:
            text: "One-Rep Max (1RM) Predictor"
            text_size: self.width, None
            halign: "center"
            bold: True
            on_release:
                app.root.current = "1rm"
                root.manager.transition.direction = "left"
    Button:
        text: "About"
        size: "60dp", "40dp"
        size_hint: None, None
        pos_hint: {"x":0.05, "top":0.95}
        bold: True
        on_release:
            app.root.current = "about"
            root.manager.transition.direction = "left"

<AboutScreen>:
    name: "about"
    BoxLayout:
        size_hint: 0.75, 0.8
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        orientation: "vertical"
        Label:
            text: "Developed by Paul Lee (10/16/2022)"
            size_hint: 1, 0.05
            bold: True
        Label:
            text: "An app for gym bros, created by a gym bro."
            size_hint: 1, 0.05
            italic: True
        Label:
            text: "Current Functions:"
            size_hint: 1, 0.1
            bold: True
        Label:
            text: "[b]Jeff Nippard's Strength Level Progression Tester[/b]:\nGives feedback about strength levels and progression based on Jeff Nippard's opinion."
            text_size: self.width, None
            size_hint: 1, 0.15
            halign: 'left'
            markup: True
        Label:
            text: "[b]Weight Breakdown[/b]:\nBreaks down barbell weight into individual plates given total weight (lb or kg)."
            text_size: self.width, None
            size_hint: 1, 0.15
            halign: 'left'
            markup: True
        Label:
            text: "[b]Plate Totals[/b]:\nDisplays total weight of the barbell given amount of plates on each side (lb or kg)."
            text_size: self.width, None
            size_hint: 1, 0.15
            halign: 'left'
            markup: True
        Label:
            text: "[b]One-Rep Max (1RM) Predictor[/b]:\nDisplays 1RM estimate given amount of weight lifted and number of reps formed."
            text_size: self.width, None
            size_hint: 1, 0.15
            halign: 'left'
            markup: True
        Label:
            text: "Special Thanks:"
            size_hint: 1, 0.1
            bold: True
        Label:
            text: "Jerronh Tam - Hype Man\nTimothy Lee - Coach"
            size_hint: 1, 0.1
    Button:
        text: "Back"
        size: "60dp", "40dp"
        size_hint: None, None
        pos_hint: {"x":0.05, "top":0.95}
        bold: True
        on_release:
            app.root.current = "main"
            root.manager.transition.direction = "right"

<NippardStrengthStandardScreen>:
    name: "nippard"
    BoxLayout:
        size_hint: 0.75, 0.75
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        orientation: "vertical"
        BoxLayout:
            size_hint: 1, 0.1
            ToggleButton:
                group: "sex"
                id: male
                text: "Male"
                bold: True
                state: "down"
            ToggleButton:
                group: "sex"
                id: female
                text: "Female"
                bold: True
        BoxLayout:
            size_hint: 1, 0.1
            Label:
                text: "Bodyweight: "
                text_size: self.width, None
                halign: "right"
                bold: True
                size_hint: 0.4, 1
            TextInput:
                id: bw
                input_filter: "float"
                hint_text: "Weight (lb)"
                size_hint: 0.6, 1
        BoxLayout:
            size_hint: 1, 0.1
            Label:
                text: "Squat: "
                text_size: self.width, None
                halign: "right"
                bold: True
                size_hint: 0.4, 1
            TextInput:
                id: s
                input_filter: "float"
                hint_text: "Weight (lb)"
                size_hint: 0.6, 1
        BoxLayout:
            size_hint: 1, 0.1
            Label:
                text: "Bench: "
                text_size: self.width, None
                halign: "right"
                bold: True
                size_hint: 0.4, 1
            TextInput:
                id: b
                input_filter: "float"
                hint_text: "Weight (lb)"
                size_hint: 0.6, 1
        BoxLayout:
            size_hint: 1, 0.1
            Label:
                text: "Deadlift: "
                text_size: self.width, None
                halign: "right"
                bold: True
                size_hint: 0.4, 1
            TextInput:
                id: d
                input_filter: "float"
                hint_text: "Weight (lb)"
                size_hint: 0.6, 1
        Button:
            size_hint: 0.5, 0.1
            text: "Calculate!"
            pos_hint: {"center_x":0.5}
            bold: True
            on_release: root.calculate()
            disabled: True if not (male.state == "down" or female.state == "down") or bw.text == "" or s.text == "" or b.text == "" or d.text == "" else False
        Label:
            text: "Total: " + str(root.total) + " lb (" + str(root.bw_times) + "x)"
            bold: True
            size_hint: 1, 0.1
        Label:
            text: "Squat Level: " + str(root.s_level_name) + " (" + str(root.s_times) + "x)\n" + str(round(100*float(root.s_progress),1)) + "% progression"
            bold: True
            size_hint: 1, 0.1
        ProgressBar:
            value_normalized: float(root.s_progress)
            size_hint: 1, 0.1
        Label:
            text: "Bench Level: " + str(root.b_level_name) + " (" + str(root.b_times) + "x)\n" + str(round(100*float(root.b_progress),1)) + "% progression"
            bold: True
            size_hint: 1, 0.1
        ProgressBar:
            value_normalized: float(root.b_progress)
            size_hint: 1, 0.1
        Label:
            text: "Deadlift Level: " + str(root.d_level_name) + " (" + str(root.d_times) + "x)\n" + str(round(100*float(root.d_progress),1)) + "% progression"
            bold: True
            size_hint: 1, 0.1
        ProgressBar:
            value_normalized: float(root.d_progress)
            size_hint: 1, 0.1
    Button:
        text: "Back"
        size: "60dp", "40dp"
        size_hint: None, None
        pos_hint: {"x":0.05, "top":0.95}
        bold: True
        on_release:
            app.root.current = "main"
            root.manager.transition.direction = "right"

<PoundsBreakdownScreen>:
    name: "pounds"
    BoxLayout:
        orientation: "vertical"
        size_hint: 0.75, 0.75
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        BoxLayout:
            size_hint: 1, 0.075
            Label:
                text: "Weight to Breakdown: "
                text_size: self.width, None
                halign: "right"
                bold: True
                size_hint: 0.4, 1
            TextInput:
